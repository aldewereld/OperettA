#Product Runtime Configuration File
eclipse.application=net.sf.ictalive.aliveclipse.application
osgi.bundles.defaultStartLevel=4
eclipse.product=net.sf.ictalive.aliveclipse.product
osgi.splashPath=platform:/base/plugins/net.sf.ictalive.aliveclipse
osgi.bundles=org.eclipse.equinox.simpleconfigurator@1:start
org.eclipse.equinox.simpleconfigurator.configUrl=file:org.eclipse.equinox.simpleconfigurator/bundles.info

# Lines below are added to generated config.ini:
# Set workspace path.
osgi.instance.area.default=@user.home/Documents/ALIVE

# when we use our own config.ini, eclipse fails to generate some directories. This line fixes the problem.
osgi.bundles=org.eclipse.equinox.common@2:start,org.eclipse.update.configurator@3:start,org.eclipse.core.runtime@start