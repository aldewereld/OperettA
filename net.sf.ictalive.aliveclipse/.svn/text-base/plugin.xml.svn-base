<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="net.sf.ictalive.aliveclipse.intro.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="net.sf.ictalive.aliveclipse.intro.Perspective"
            icon="icons/aliveFile.gif"
            id="ALIVEclipse.perspective"
            name="ALIVE">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="net.sf.ictalive.aliveclipse.application"
            description="Integrated Design Tools for ALIVE Systems"
            name="ALIVEclipse">
         <property
               name="appName"
               value="ALIVEclipse">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="350,40,140,15">
         </property>
         <property
               name="startupProgressRect"
               value="350,25,140,15">
         </property>
         <property
               name="windowImages"
               value="icons/icon_16x16x32.png,icons/icon_32x32x32.png,icons/icon_48x48x32.png,icons/icon_128x128x32.png">
         </property>
         <property
               name="aboutImage"
               value="icons/about.gif">
         </property>
         <property
               name="aboutText"
               value="ALIVEclipse is a product of the ALIVE consortium. The research and development has been carried out within the FP7-215890 ALIVE project, funded by the European Commission.&#x0A;&#x0A;Copyright © 2008-2010 EU ALIVE project.&#x0A;&#x0A;Product partners:&#x0A;Universitat Politècnica de Catalunya&#x0A;University of Bath&#x0A;Calico Jack Ltd.&#x0A;Trinity College Dublin&#x0A;Thales Nederland B.V.&#x0A;Tech Media Telecom Factory SL&#x0A;The University Court of the University of Aberdeen&#x0A;Universiteit Utrecht.">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.intro">
      <intro
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            icon="icons/welcome.png"
            id="aliveclipse.intro">
      </intro>
      <introProductBinding
            introId="aliveclipse.intro"
            productId="net.sf.ictalive.aliveclipse.product">
      </introProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            introId="aliveclipse.intro"
            content="introContent.xml"
            id="ALIVEclipse.configId">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx">
            </implementation>
         </presentation>
      </config>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            content="ext.xml"
            configId="ALIVEclipse.configId">
      </configExtension>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="ALIVEclipse.category.wizards"
            name="ALIVE">
      </category>
      <wizard
            category="ALIVEclipse.category.wizards"
            class="net.sf.ictalive.aliveclipse.NewProjectWizard"
            finalPerspective="ALIVEclipse.perspective"
            icon="icons/Alive_16x16.gif"
            id="net.sf.ictalive.aliveclipse.NewProjectWizardID"
            name="Alive Project"
            project="true">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="net.sf.ictalive.aliveclipse.AliveNavigator"
            icon="icons/eclipseNavigator.gif"
            category = "org.eclipse.ui"
         	id="net.sf.ictalive.navigator"
            name="ALIVE Navigator">
      </view>
   </extension>
      <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:showView?before=showViewSeparator">
            <command
                  commandId="org.eclipse.ui.views.showView"
                  label="ALIVE Navigator"
                  icon = "icons/eclipseNavigator.gif"
         	      style="push">
               <parameter
                     name="org.eclipse.ui.views.showView.viewId"
                     value="net.sf.ictalive.navigator">
               </parameter>
            </command>
      </menuContribution>
   </extension>
   
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding
            viewerId="net.sf.ictalive.navigator">
         <includes>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.*">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="net.sf.ictalive.navigator">
         <includes>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.linkHelper">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.workingSets">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>

   <extension
         id="net.sf.ictalive.aliveclipse.projectNature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="net.sf.ictalive.aliveclipse.ProjectNature">
         </run>
      </runtime>
   </extension>


   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:openPerspective?before=openPerspectiveSeparator">
         <command
                  commandId="org.eclipse.ui.perspectives.showPerspective"
                  label="ALIVE Perspective"
                  style="radio">
               <parameter
                     name="org.eclipse.ui.perspectives.showPerspective.perspectiveId"
                     value="ALIVEclipse.perspective">
               </parameter>
            </command>
         
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/Alive_16x16.gif"
            id="net.sf.ictalive.aliveclipse.projectNatureImageID"
            natureId="net.sf.ictalive.aliveclipse.projectNature">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonWizard
            type="new"
            wizardId="net.sf.ictalive.aliveclipse.NewProjectWizardID">
         <enablement></enablement>
      </commonWizard>
      <commonWizard
            type="new"
            wizardId="net.sf.ictalive.operetta.OM.presentation.OMModelWizardID">
         <enablement></enablement>
      </commonWizard>
   </extension>
            
 
</plugin>
